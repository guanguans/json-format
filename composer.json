{
    "name": "guanguans/json-format",
    "keywords": [
        "json",
        "format",
        "pretty"
    ],
    "description": "Format json string.",
    "support": {
        "issues": "https://github.com/guanguans/json-format/issues",
        "source": "https://github.com/guanguans/json-format"
    },
    "type": "library",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "guanguans",
            "email": "ityaozm@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4|~5|~6|~7|~8|~9"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "Guanguans\\JsonFormat\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Guanguans\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --coverage-text --color=always --debug --verbose"
    },
    "scripts-descriptions": {
        "test": "Run the phpunit test."
    }
}